local function Billboard(child, name, color, size, title)
local bill = Instance.new("BillboardGui",child) do
local txt = Instance.new("TextLabel",bill)
local uistroke = Instance.new("UIStroke", txt)
        bill.AlwaysOnTop = true
        bill.Size = UDim2.new(0,400,0,100)
        bill.Adornee = child
        bill.Name = title
        bill.Active = true
        bill.AlwaysOnTop = true
        txt.AnchorPoint = Vector2.new(0.5,0.5)
        txt.BackgroundTransparency = 1
        txt.BackgroundColor3 = Color3.new(1,1,1)
        txt.TextColor3 = color
        txt.Font = "RobotoMono"
        txt.TextSize = size
        txt.Text = name
        txt.TextTransparency = 0
        txt.Visible = true
        txt.Size = UDim2.new(1,0,0,0)
        txt.Position = UDim2.new(0.5,0,0.7,-35)
        uistroke.Thickness = 1.25
        uistroke.Color = Color3.new(0,0,0)
      end
end
local function Highlight(child, name, color, size, title)
Billboard(child, name, color, size, title)
local Highlight = Instance.new("Highlight") do
        Highlight.FillTransparency = 0.75
        Highlight.OutlineTransparency = 0
        Highlight.Name = title
        Highlight.Adornee = child
        Highlight.Parent = child
        Highlight.FillColor = color
        Highlight.OutlineColor = color
	if child:IsA("Part") then
        child.Transparency = 0
	end
      end
end
local function Clear(name)
for _,v in pairs(workspace:GetDescendants()) do
if v.Name == name then
v:Destroy()
end
end
end
